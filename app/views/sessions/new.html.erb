  <%= render 'form' %>
<div class="picture_wrapper">
  <div class="form" id="login">
  <h2>Sign In</h2>
</br>
  <div class="login">
    <p style="color: red"><%= flash[:error] %></p>
    <%= form_tag(session_path) do %>
      <%= text_field_tag :email, nil, placeholder: 'Email' %><br/>
      <%= password_field_tag :password, nil, placeholder: 'Password' %><br/><br/>
      <%= submit_tag('Login') %>
    <% end %>
  </div>
  <button ><%= link_to 'Sign up', users_path, id: 'sign_up_button', method: :post %></button>
  </div>
<div class="split_it">Split.it</div>
</div>


