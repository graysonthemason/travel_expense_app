<header>
  <img class="header_image" src="<%=current_user.image%>" width=30px height=30px %>
    <span class="logout"><%= link_to("Logout", session_path, method: :delete) %></span>
    <% if current_user.is_admin? %>
    <span class="admin">Admin</span>
      <span class="admin_a"><%= link_to('Users', users_path) %>
      </span>
    <% end %>
    <% if session.id %>
      <span class="logout"><%=
       link_to("#{current_user.name}", user_path(id: session.id)) %></span>

	<% end %>
</header>
